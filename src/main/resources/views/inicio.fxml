      </Label>
      <Label layoutX="278.0" layoutY="170.0" text="Que desea registrar...?">
         <font>
            <Font size="18.0" />
         </font></Label>
      <GridPane layoutX="232.0" layoutY="266.0" prefHeight="159.0" prefWidth="269.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnClientes" mnemonicParsing="false" onAction="#clientesEvent" prefHeight="57.0" prefWidth="129.0" text="Clientes">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="btnProductos" mnemonicParsing="false" onAction="#productosEfvent" prefHeight="57.0" prefWidth="129.0" text="Productos" GridPane.columnIndex="1">
